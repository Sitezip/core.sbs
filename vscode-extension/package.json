{
  "name": "corejs-snippets",
  "displayName": "core.js Snippets",
  "description": "Code snippets for the core.js framework - Component Oriented Rendering Engine",
  "version": "1.1.0",
  "publisher": "corejs",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "core.js",
    "corejs",
    "snippets",
    "javascript",
    "html",
    "framework",
    "component",
    "rendering",
    "pocket",
    "template",
    "clone"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sitezip/core.sbs"
  },
  "homepage": "https://core.sbs",
  "bugs": {
    "url": "https://github.com/Sitezip/core.sbs/issues"
  },
  "license": "MIT",
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "source.corejs.embedded",
        "path": "./syntaxes/corejs.tmLanguage.json",
        "injectTo": [
          "text.html.basic",
          "text.html.derivative",
          "source.js"
        ]
      }
    ],
    "themes": [
      {
        "label": "core.js Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/corejs-dark.json"
      }
    ]
  }
}
